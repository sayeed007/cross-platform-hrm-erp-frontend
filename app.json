{
  "expo": {
    "name": "cross-platform-hrm-erp-frontend",
    "slug": "cross-platform-hrm-erp-frontend",
    "extra": {
      "API_URL": "http://hrm.tafuritechnologies.com:49002",
      "eas": {
        "projectId": "9a5c40ab-0107-436a-b6f9-7eeec8e9179b"
      }
    },
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/Tafuri-hrms-logo-animation.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#0F75BC"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.shb007.crossplatformhrmerpfrontend"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/Tafuri-hrms-logo-animation.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.shb007.crossplatformhrmerpfrontend",
      "permissions": [
        "RECEIVE_BOOT_COMPLETED"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "experiments": {
      "typedRoutes": true
    },
    "plugins": [
      [
        "expo-build-properties",
        {
          "android": {
            "usesCleartextTraffic": true
          }
        }
      ],
      "expo-font"
    ]
  }
}
